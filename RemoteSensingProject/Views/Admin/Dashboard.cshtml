<script>
    $(document).ready(function () {

        $.ajax({
            url: "/Admin/BindOverallCompletionPercentage",
            type: "GET",
            success: function (res) {
                // Clear existing chart content
                $("#monthlySalesChart").empty();

                var categories = [];
                var data = [];

                $.each(res.list, function (index, item) {
                    categories.push(item.Title );
                    data.push(item.OverallCompletionPercentage);
                });

                var options = {
                    chart: {
                        type: 'bar',
                        height: '318',
                        parentHeightOffset: 0,
                        foreColor: '#6c757d',
                        background: '#ffffff',
                        toolbar: {
                            show: false
                        },
                    },
                    theme: {
                        mode: 'light'
                    },
                    tooltip: {
                        theme: 'light'
                    },
                    colors: ['#1aa7a6'],
                    fill: {
                        opacity: .9
                    },
                    grid: {
                        padding: {
                            bottom: -4
                        },
                        borderColor: '#D6E9E9',
                        xaxis: {
                            lines: {
                                show: true
                            }
                        }
                    },
                    series: [{
                        name: 'Completed',
                        data: data // Dynamic data
                    }],
                    xaxis: {
                        type: 'category',
                        categories: categories, // Dynamic categories
                        axisBorder: {
                            color: '#dee2e6',
                        },
                        axisTicks: {
                            color: '#dee2e6',
                        },
                    },
                    yaxis: {
                        min: 0,
                        max: 100,
                        tickAmount: 5,
                        title: {
                            text: 'Project Completion',
                            style: {
                                fontSize: '12px',
                                color: '#adb5bd',
                                fontFamily: 'inherit',
                            }
                        },
                    },
                    legend: {
                        show: true,
                        position: "top",
                        horizontalAlign: 'center',
                        fontFamily: 'inherit',
                        itemMargin: {
                            horizontal: 8,
                            vertical: 0
                        },
                    },
                    stroke: {
                        width: 0
                    },
                    dataLabels: {
                        enabled: true,
                        style: {
                            fontSize: '10px',
                            fontFamily: 'inherit',
                        },
                        offsetY: -27
                    },
                    plotOptions: {
                        bar: {
                            columnWidth: "50%",
                            borderRadius: 4,
                            dataLabels: {
                                position: 'top',
                                orientation: 'vertical',
                            }
                        },
                    },
                };

                // Render the chart
                var apexBarChart = new ApexCharts(document.querySelector("#monthlySalesChart"), options);
                apexBarChart.render();
            }
        });




        if ($('#storageChart').length) {
            var colors = {
                primary: '#029e9d', // Set the primary color to #029e9d
                light: '#f3f3f3',   // You can define other colors like light, muted, etc.
